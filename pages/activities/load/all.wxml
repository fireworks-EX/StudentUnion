<view class="container">
  <van-collapse wx:for="{{ result.dataObj }}" value="{{ activeName }}" bind:change="onChange" accordion>
  <van-collapse-item title="活动名：{{item.activityname}}" name="{{index}}">
    <text>活动id: {{item.activityid}}\n申请部门：{{item.applydep}}\n协同部门：{{item.departmentassist}}\n需要人数: {{item.peopleamount}}\n开始时间：{{item.starttime}}\n结束时间：{{item.overtime}}\n审批id：{{item.judgeid}}\n状态（是否取消）：{{item.iscancel}}</text>
  </van-collapse-item>
</van-collapse>
</view>