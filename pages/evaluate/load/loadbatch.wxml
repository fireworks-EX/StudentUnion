<view class="container">
<van-panel title="考核批次"  status="详情">
  <van-cell-group wx:for="{{result.dataObj}}">
    <van-cell is-link link-type="navigateTo" url="load" >
      <view slot="title" id="{{index}}" bindtap="click">
        <text class="txt">批次id：{{item.batchid}}\n结束时间：{{item.endtime}}</text>
      </view>
    </van-cell>
  </van-cell-group>
</van-panel>
</view>